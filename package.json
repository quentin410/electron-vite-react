{
  "name": "electron-vite-react",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "build/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev nodemon --watch build --exec \"electron build/main.js\"",
    "dev": "concurrently \"tsc -w\" \"npm start\" \"npm run clientStart\"",
    "serverStart": "concurrently \"tsc -w\" \"npm start\"",
    "clientStart": "cd client && npm run clientStart",
    "clientBuild": "cd client && npm run build",
    "buildMac": "npm run clientBuild  && tsc && electron-builder build --mac",
    "justbuildForMac": "electron-builder build --mac",
    "f2elint-scan": "f2elint scan",
    "f2elint-fix": "f2elint fix"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "react",
    "vite",
    "tutorial",
    "demo"
  ],
  "author": "quentin410",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/parser": "^7.14.3",
    "@babel/traverse": "^7.14.2",
    "@types/babel-traverse": "^6.25.5",
    "@types/babel__traverse": "^7.11.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/ejs": "^3.0.6",
    "@types/node": "^14.14.41",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "devtron": "^1.4.0",
    "electron-builder": "22.10.5",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.1.1",
    "f2elint": "^2.1.0",
    "less": "^4.1.1",
    "less-loader": "^8.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vite": "^2.1.5"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^1.1.3",
    "@viterjs/vite-plugin-react-router-config": "^1.0.0",
    "camelcase": "^6.2.0",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "ejs": "^3.1.6",
    "electron": "^16.0.5",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^2.0.0",
    "koa": "^2.13.1",
    "koa-body-parser": "^1.1.2",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "nodemon": "^2.0.7",
    "open": "^8.0.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.2.0",
    "style-loader": "^2.0.0",
    "styled-components": "^5.3.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vite-plugin-style-import": "^1.4.0"
  },
  "build": {
    "productName": "electron-vite-react",
    "buildVersion": "0001",
    "appId": "",
    "copyright": "Copyright Â© 2021 ${author}",
    "files": [
      "build/**/*",
      "package.json",
      "preload.js",
      "server/public/**/*"
    ],
    "asar": true,
    "artifactName": "${productName}-${version}.${ext}",
    "compression": "maximum",
    "publish": [
      {
        "provider": "generic"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "category": "",
      "target": [
        "dmg"
      ],
      "extendInfo": {
        "ATSApplicationFontsPath": "Fonts",
        "NSRequiresAquaSystemAppearance": true,
        "ElectronTeamID": ""
      }
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "f2elint commit-file-scan",
      "commit-msg": "f2elint commit-msg-scan"
    }
  }
}
